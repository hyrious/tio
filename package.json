{
    "name": "tio",
    "displayName": "Tio",
    "description": "Run any language on tio.run",
    "icon": "icon.png",
    "publisher": "hyrious",
    "version": "0.0.3",
    "repository": {
        "type": "git",
        "url": "https://github.com/hyrious/tio.git"
    },
    "engines": {
        "vscode": "^1.46.0"
    },
    "activationEvents": [
        "onCommand:tio.run",
        "onCommand:tio.run2"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "tio.run",
                "title": "Tio: Run"
            },
            {
                "command": "tio.run2",
                "title": "Tio: Run with Input and Args"
            }
        ]
    },
    "devDependencies": {
        "@types/got": "^9.6.11",
        "@types/vscode": "^1.46.0"
    },
    "dependencies": {
        "got": "^11.8.5"
    }
}
